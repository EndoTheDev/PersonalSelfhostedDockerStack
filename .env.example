# Cloudflare Tunnel Token - Get from Cloudflare Zero Trust dashboard
TUNNEL_TOKEN=your_cloudflare_tunnel_token_here

# Homarr Dashboard Encryption Key - Generate a random 32-character hex string
SECRET_ENCRYPTION_KEY=your_random_32_char_hex_key_here

# Traefik Dashboard Basic Auth - Generate with: htpasswd -nb username password
BASIC_AUTH=admin:$apr1$encrypted_password_here

# Tududi Task Management
TUDUDI_USER_EMAIL=your_email@example.com
TUDUDI_USER_PASSWORD=your_secure_password
TUDUDI_SESSION_SECRET=your_32_character_random_session_secret

# Pi-hole
PIHOLE_API_PASSWORD=your_secure_api_password_here

# Redis Configuration
REDIS_URL=redis://redis:6379
REDIS_DB_WEBUI=1
REDIS_DB_SEARCH=0

# N8n Configuration
N8N_HOST=n8n.${DOMAIN}
N8N_PROTOCOL=https
WEBHOOK_URL=https://n8n.${DOMAIN}/
N8N_EDITOR_BASE_URL=https://n8n.${DOMAIN}/

# Root domain for all services (e.g., example.com)
DOMAIN=yourdomain.com